/******************************************************************************

На вход подаётся целое знаковое число n.
Описать функцию, которая выводит (если возможно) любой набор из 3 чисел [a,b,c],
произведение которых равняется заданному числу: a*b*c=n. Коэффициенты по модулю должны быть больше 1.

*******************************************************************************/
#include <iostream>
#include <cmath>
using namespace std;

void a(int n)
{
    int flag = 0;
    for (int i = 2; i < ceil(n/2); i++)
    {
        
        for (int j = 2; j < ceil(n/2); j++)
        {
            
            for (int k = 2; k < ceil(n/2); k++)
            {
                
                if (i * j * k == n)
                    {
                        cout << i << " " << j << " " << k;
                        flag = 1;
                        break;
                    }
                    
            }
            
            if (flag == 1)
                break;
        }
    
        if (flag == 1)
            break;
    }
    
    

   
}
int main()
{
    int n;
    cin >> n;
    a(n);
}
